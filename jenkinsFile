pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                    sh 'mvn clean package'
            }
        }
         stage('Run') {
            steps {
                 sh 'sudo nohup java -jar target/jenkins-closing-task-0.0.1-SNAPSHOT.jar &'
            }
          }
     }

    post{
        success{
slackSend baseUrl: 'https://hooks.slack.com/services/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/', channel: 'jenkins-final-task-notifacation', message: 'build success', teamDomain: 'jenkins'        }
    
    }      
}
