pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                    sh 'mvn clean package'
            }
        }
         stage('Run') {
            steps {
                 sh 'sudo nohup java -jar target/jenkins-closing-task-0.0.1-SNAPSHOT.jar &'
            }
          }
     }

    post{
        success{
            slackSend baseUrl: 'https://hooks.slack.com/services/', channel: '#jenkins-final-task-notifacation', message: 'built success!!', teamDomain: 'jenkins', tokenCredentialId: 'slack'}
        }
    
    }      
}
